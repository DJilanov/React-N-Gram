{
  "extends": "airbnb-base",
  "rules": {
    "linebreak-style": [
      0
    ],
    "global-require": [
      0
    ],
    "no-console": [
      0
    ],
    "camelcase": [
      0
    ],
    "class-methods-use-this": [
      0
    ],
    "no-prototype-builtins": [
      0
    ],
    "max-len": [
      2,
      {
        "code": 155,
        "ignoreUrls": true,
        "ignoreStrings": true,
        "ignoreTemplateLiterals": true
      }
    ],
    "object-curly-newline": [
      0,
      {
        "ObjectExpression": "never",
        "ImportDeclaration": "never",
        "ExportDeclaration": {
          "multiline": true,
          "minProperties": 3
        }
      }
    ]
  }
}